			<groupId>com.ailk.product.itms.app</groupId>
			<artifactId>ailk-itms-tr069</artifactId>
			<version>1.0-SNAPSHOT</version>
			<exclusions>
				<exclusion>
					<groupId>com.ailk</groupId>
					<artifactId>linkage-commons</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>oracle.jdbc</groupId>
			<artifactId>oracle-jdbc</artifactId>
		</dependency>
		<dependency>
			<groupId>sybase.jdbc</groupId>
			<artifactId>sybase-jdbc-jconn2</artifactId>
		</dependency>
		<!-- jdbc driver (optional) -->
